<div class="container">
	<div class="row">

		<%= render 'users/function', book:@book, user:@user %>

		<div class="col-xs-9">
		<h1>Users</h1>
		<div>
		<table class="table table-striped">
			<thead>
				<tr>
					<th>image</th>
					<th>name</th>
					<th></th>
				</tr>
			</thead>

			<% @users.each do |f| %>
				<tbody>
					<tr>
						<td> <%= attachment_image_tag f, :profile_image, :fill, 60, 60, format:'jpeg', fallback: "no_image.jpg", size:'40x40' %></td>
						<td><%= f.name %></td>
						<td><%= link_to 'Show', user_path(f) %></td>
					</tr>
				</tbody>
			<% end %>

		</table>


		</div>
	</div>
</div>
